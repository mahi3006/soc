#include <bits/stdc++.h>
using namespace std;
int main() {
    int n;
    cin >> n;
    vector<pair<int, int>> v(n);
    for (auto &[a, b] : v) cin >> a >> b;
    sort(v.begin(), v.end());
    long long t = 0;
    for (auto [a, b] : v) {
        t += a;
        if (t > b) {
            cout << "NO\n";
            return 0;
        }
    }
    cout << "YES\n";
}
