#include <bits/stdc++.h>
using namespace std;
#define ll long long

ll gcd(ll a, ll b) {
    while (b) tie(a, b) = make_pair(b, a % b);
    return a;
}

int main() {
    ll a, b;
    cin >> a >> b;
    if (a == b) {
        cout << -1 << '\n';
        return 0;
    }

    ll d = abs(a - b);
    ll ans = LLONG_MAX;

    for (ll i = 1; i * i <= d; ++i) {
        if (d % i == 0) {
            for (ll f : {i, d / i}) {
                ll rem = a % f;
                ll x = rem == 0 ? 0 : f - rem;
                if (gcd(a + x, b + x) > 1) {
                    ans = min(ans, x);
                }
            }
        }
    }

    cout << ans << '\n';
}
